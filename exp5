#include <stdio.h>

int main() {
    char ch;
    int spaces = 0, newlines = 0;

    printf("Enter text (end with #):\n");

    while ((ch = getchar()) != '#') {
        if (ch == ' ')
            spaces++;
        else if (ch == '\n')
            newlines++;
    }

    printf("Number of whitespaces = %d\n", spaces);
    printf("Number of newlines = %d\n", newlines);

    return 0;
}
